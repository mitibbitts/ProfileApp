{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "rating": {
        "type": "integer"
      },
      "status": {
        "type": "string",
        "enum": ["sad", "Vibing"]
      }
    },
    "required": ["name", "status"],
    "additionalProperties": false
  }
}
